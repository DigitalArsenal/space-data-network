{
  "name": "@spacedatanetwork/spaceaware",
  "version": "0.1.0",
  "private": true,
  "description": "SpaceAware Svelte 5 single-file homepage powered by OrbPro + SDN APIs",
  "type": "module",
  "scripts": {
    "dev": "vite dev --host 0.0.0.0 --port 4180",
    "build": "vite build && node scripts/inline-build.mjs",
    "build:ipfs": "npm run build && node scripts/copy-orbpro-build.mjs",
    "preview": "vite preview --host 0.0.0.0 --port 4180",
    "clean": "rm -rf dist .vite",
    "serve": "npx http-server dist -p 4180 -c-1"
  },
  "keywords": [
    "spaceaware",
    "orbpro",
    "single-file",
    "sdn",
    "ipfs",
    "svelte"
  ],
  "license": "MIT",
  "dependencies": {
    "svelte": "^5.50.1"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^5.1.1",
    "vite": "^6.4.1"
  }
}
